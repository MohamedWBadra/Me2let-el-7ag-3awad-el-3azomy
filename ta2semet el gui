ta2semet el gui ya gd3an yarab n5ls
el moshtarak le kolo:{
  by5tar login wla sign up(omar)
  login(omar)
  signup(omar)
  view:
  view all(hadary)
  view by category(nafs el view all ta2reban)(hadary)
  search product(hadary)
}
customer:(mizo){
  cart:
    view
    add(mawgoda lma yview el products 3mtan)
    remove(lma y33ml view cart)
  place order:
    view total price
    payment 3moman bkol 5waze2o
}
admin:(badra){
  add supplier(ta2reban nfs el login bs bshkl mo5tlef seka)
  Ban customer(btesta3ml fel view customer)
  views:
    view admins
    view orders
    view suppliers
    view customer
}
supplier:
  add product(omar)

  
